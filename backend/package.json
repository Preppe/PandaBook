{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create": "hygen seeds create",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "11.0.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.12",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/platform-socket.io": "^11.0.12",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "11.1.0",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "^11.0.12",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/multer-s3": "3.0.3",
    "apple-signin-auth": "1.7.8",
    "bcryptjs": "3.0.2",
    "bull": "^4.16.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fb": "2.0.0",
    "google-auth-library": "9.15.1",
    "handlebars": "4.7.8",
    "ioredis": "^5.6.0",
    "ms": "2.1.3",
    "multer": "1.4.4",
    "multer-s3": "3.0.1",
    "music-metadata": "^11.0.2",
    "nestjs-i18n": "10.5.1",
    "nestjs-paginate": "^11.1.1",
    "nodemailer": "6.10.0",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "pg": "8.14.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "socket.io-redis": "^6.1.1",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "5.0.1",
    "twitter": "1.7.1",
    "typeorm": "0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.2",
    "@nestjs/testing": "11.0.12",
    "@types/bcryptjs": "3.0.0",
    "@types/express": "5.0.1",
    "@types/facebook-js-sdk": "3.3.12",
    "@types/jest": "29.5.14",
    "@types/ms": "2.1.0",
    "@types/multer": "^1.4.12",
    "@types/node": "22.13.14",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.3",
    "@types/twitter": "1.7.4",
    "env-cmd": "10.1.0",
    "husky": "9.1.7",
    "hygen": "6.2.11",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "ts-jest": "29.3.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typescript": "5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
